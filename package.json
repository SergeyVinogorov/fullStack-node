    "celebrate": "^15.0.0",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.7"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "jest": "^27.2.2",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6"
  }
